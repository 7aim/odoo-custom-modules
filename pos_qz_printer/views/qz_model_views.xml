<odoo>
    <record id="view_qz_form" model="ir.ui.view">
        <field name="name">qz.print.test.form</field>
        <field name="model">qz.print.test</field>
        <field name="arch" type="xml">
            <form string="QZ Yazıcı Testi">
                <header>
                    <button name="action_print_receipt"
                            string="Fişi Yazdır"
                            type="object"
                            class="btn-primary"/>
                </header>
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="total"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="action_qz_test" model="ir.actions.act_window">
        <field name="name">QZ Yazıcı Test</field>
        <field name="res_model">qz.print.test</field>
        <field name="view_mode">list,form</field>
    </record>

    <menuitem id="menu_qz_root" name="QZ Yazdırma" sequence="10"/>
    <menuitem id="menu_qz_test" name="Fiş Testi" parent="menu_qz_root" action="action_qz_test"/>
</odoo>
